{
  "query": {
    "bool": {
      "must": [
        {
          "query_string": {
            "analyze_wildcard": true,
            "query": "name:affirm.a.metric"
          }
        },
        {
          "range": {
            "@timestamp": {
              "gte": "now-100m"
            }
          }
        }
      ]
    }
  },
  "aggs": {
    "agg": {
      "terms": {
        "field": "grouper",
        "order": {
          "1": "desc"
        },
        "min_doc_count": 1,
        "size": 20
      },
      "aggs": {
        "1": {
          "avg": {
            "field": "timing"
          }
        },
        "agg": {
          "terms": {
            "field": "flounder",
            "order": {
              "_term": "desc"
            },
            "min_doc_count": 1,
            "size": 500
          },
          "aggs": {
            "agg": {
              "date_histogram": {
                "field": "@timestamp",
                "interval": "20m",
                "extended_bounds": {
                  "max": "now",
                  "min": "now-100m"
                }
              },
              "aggs": {
                "avg": {
                  "avg": {
                    "field": "timing"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
